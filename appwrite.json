{
    "projectId": "crm",
    "projectName": "Personal CRM",
    "settings": {
        "services": {
            "account": true,
            "avatars": true,
            "databases": true,
            "locale": true,
            "health": true,
            "storage": true,
            "teams": true,
            "users": true,
            "sites": true,
            "functions": true,
            "graphql": true,
            "messaging": true
        },
        "auth": {
            "methods": {
                "jwt": true,
                "phone": true,
                "invites": true,
                "anonymous": true,
                "email-otp": true,
                "magic-url": true,
                "email-password": true
            },
            "security": {
                "duration": 31536000,
                "limit": 0,
                "sessionsLimit": 10,
                "passwordHistory": 0,
                "passwordDictionary": false,
                "personalDataCheck": false,
                "sessionAlerts": false,
                "mockNumbers": []
            }
        }
    },
    "databases": [
        {
            "$id": "db",
            "name": "DB",
            "enabled": true
        }
    ],
    "collections": [
        {
            "$id": "crm",
            "$permissions": [],
            "databaseId": "db",
            "name": "CRM",
            "enabled": true,
            "documentSecurity": false,
            "attributes": [
                {
                    "key": "name",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "email",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "format": "email",
                    "default": null
                },
                {
                    "key": "phone",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 25,
                    "default": null
                },
                {
                    "key": "notes",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 2000,
                    "default": null
                }
            ],
            "indexes": []
        }
    ],
    "buckets": [
        {
            "$id": "685ec4b50010f7e812a5",
            "$permissions": [],
            "fileSecurity": false,
            "name": "CSV",
            "enabled": true,
            "maximumFileSize": 50000000,
            "allowedFileExtensions": [],
            "compression": "none",
            "encryption": true,
            "antivirus": true
        }
    ],
    "functions": [
        {
            "$id": "db-operations",
            "name": "database-operations",
            "runtime": "node-22",
            "specification": "s-0.5vcpu-512mb",
            "execute": [
                "any"
            ],
            "events": [],
            "scopes": [
                "documents.read",
                "documents.write"
            ],
            "schedule": "",
            "timeout": 15,
            "enabled": true,
            "logging": true,
            "entrypoint": "src/main.js",
            "commands": "npm install",
            "path": "functions/database-operations"
        }
    ]
}